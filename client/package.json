{
  "name": "client",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:css && npm run build:js",
    "build:css": "NODE_ENV=production postcss ./node_modules/tailwindcss/tailwind.css -o dist/kament.css --verbose && du -sh dist/kament.css",
    "build:js": "esbuild kament.jsx --bundle '--define:process.env.NODE_ENV=\"production\"' --minify --outfile=dist/kament.js --summary"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "esbuild": "^0.8.42",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.2"
  },
  "dependencies": {
    "date-fns": "^2.17.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
